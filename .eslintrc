{
  "root": true,
  "extends": ["airbnb", "plugin:jsx-a11y/recommended"],
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "react/jsx-no-undef": "off",
    "no-console": "off",
    "jsx-a11y/anchor-is-valid": 0,
    "react/no-children-prop": 0
  },
  "plugins": ["react", "jsx-a11y", "import"]
}
