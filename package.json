{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://soybruno.com",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.22",
    "@fortawesome/free-regular-svg-icons": "^5.10.2",
    "@fortawesome/free-solid-svg-icons": "^5.10.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "jquery": "^3.4.1",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-ga": "^2.6.0",
    "react-particles-js": "^2.7.0",
    "react-scripts": "^3.1.0",
    "react-text-loop": "^2.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "GENERATE_SOURCEMAP=false && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "npx eslint --ext .jsx,.js src",
    "clean-bucket": "aws s3 rm s3://soybruno.com --recursive",
    "sync-bucket": "aws s3 sync build/ s3://soybruno.com",
    "deploy": "npm run build && npm run clean-bucket && npm run sync-bucket"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.3",
    "prettier-eslint": "^9.0.0"
  }
}
