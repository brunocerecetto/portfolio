{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://soybruno.com",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-regular-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "i18next": "^19.4.1",
    "i18next-browser-languagedetector": "^4.0.2",
    "i18next-xhr-backend": "^3.2.2",
    "jquery": "^3.5.0",
    "node-sass": "^4.13.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-ga": "^2.7.0",
    "react-i18next": "^11.3.4",
    "react-particles-js": "^2.7.0",
    "react-scripts": "^3.3.0",
    "react-scroll": "^1.7.14",
    "react-spring": "^8.0.27",
    "react-sticky": "^6.0.3",
    "react-text-loop": "^2.2.1",
    "snyk": "^1.319.1"
  },
  "scripts": {
    "start": "npm-run-all --parallel watch:css start:react",
    "build": "npm-run-all build:css build:react",
    "build:css": "postcss src/assets/styles/tailwind.css -o src/index.css",
    "watch:css": "postcss src/assets/styles/tailwind.css -o src/index.css -w",
    "start:react": "react-scripts start",
    "build:react": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "npx eslint --ext .jsx,.js src",
    "clean-bucket": "aws s3 rm s3://soybruno.com --recursive",
    "sync-bucket": "aws s3 sync build/ s3://soybruno.com",
    "deploy": "npm run build && npm run clean-bucket && npm run sync-bucket",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "autoprefixer": "^9.7.1",
    "babel-eslint": "^10.0.3",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "husky": "^3.0.9",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "prettier-eslint": "^9.0.0",
    "tailwindcss": "^1.1.3"
  },
  "snyk": true
}
